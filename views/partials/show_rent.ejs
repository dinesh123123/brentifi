<style>#line_chart {  
    height: 1px;
     margin-top: opx;
     margin-left: 25px; }
     </style>

      <section class="content dashboard">
            <div class="main-graph">
                    <div class="row clearfix">
                        <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                            <div class="graph-area">
                                 <div id="line_chart"></div>
                            </div> 
                        </div>
                      
                    </div>
                </div>
                <!-- #END# Main Graph-->
            <div class="page-heading">
                <h1>Orders</h1>
                <ol class="breadcrumb">
                    <li><a href="/public/index">Home</a></li>
                    <li class="active">orders</li>
                </ol>
            </div>


            <div class="panel panel-default">
            
           <div class="panel-heading"></div>
           
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table id="myTable" class="table js-exportable">
                            
                            <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Customers</th>
                                        <th>Email</th>
                                        <th>phone</th>
                                        <th>Address</th>
                                        <th>City</th>
                                        <th>Pincode</th>
                                        <th>Order date</th>
                                        <th>Details</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
            <% for(var i=0; i < data.length; i++) { %>
                                   
        <tr>
         <td><%= i+1%></td>
         <td><%=data[i].userId.name%></td>
        
        
 
 <td><%=data[i].userId.email%></td>
 <td><%=data[i].userId.phone%></td>
 
 



 <td><%=data[i].ShippingAddressId.address%></td>
 <td><%=data[i].ShippingAddressId.city%></td>
 <td><%=data[i].ShippingAddressId.pincode%></td>
 <td><%=data[i].order_date%></td>

 <td><a href="/public/order_details/<%=data[i].id%>" class="btn btn-primary">view</a></td>
                                        </tr>
                                        <% } %>
                                     
                               </tbody> 
                        
                            </table>
                        </div>
                    </div>
                </div>
                <!-- #END# Task data -->
            </div>
        </section>